{
  "name": "@axis/surveillance-interface",
  "description": "AXIS Surveillance System Frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config config/build/vite.config.ts",
    "build": "vue-tsc --noEmit --project config/types/tsconfig.json && vite build --config config/build/vite.config.ts",
    "preview": "vite preview --config config/build/vite.config.ts",
    "type-check": "vue-tsc --noEmit --project config/types/tsconfig.json",
    "lint": "eslint . --config config/lint/eslint.config.js --fix",
    "format": "prettier --config config/lint/.prettierrc.json --write \"src/**/*.{js,ts,tsx,vue,css}\"",
    "lint:check": "eslint . --config config/lint/eslint.config.js",
    "format:check": "prettier --config config/lint/.prettierrc.json --check \"src/**/*.{js,ts,tsx,vue,css}\"",
    "storybook": "NODE_NO_WARNINGS=1 storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest --config config/test/vitest.config.ts",
    "test:ui": "vitest --config config/test/vitest.config.ts --ui",
    "test:watch": "vitest --config config/test/vitest.config.ts --watch",
    "test:coverage": "vitest --config config/test/vitest.config.ts --coverage"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/vue-table": "^8.21.3",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.9.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-vue": "^8.6.0",
    "hls.js": "^1.6.13",
    "lucide-vue-next": "^0.542.0",
    "pinia": "^2.2.6",
    "reka-ui": "^2.5.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.8",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.18",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.8",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.34.0",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-links": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/vue3-vite": "^9.1.3",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^9.1.3",
    "eslint-plugin-vue": "^10.4.0",
    "happy-dom": "^20.0.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "storybook": "^9.1.3",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.0"
  }
}
